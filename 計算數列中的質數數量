def prime(num):
    if num > 1:
        for i in range(2, int(num ** 0.5) + 1):
           if (num % i) == 0:
                return False
        return True
    else:
        return False

while True:
    a = 0
    num = list(map(int, input().split()))                   
    for i in num:
        if prime(i) == False:
            a += 0
        else:
            a += 1
    print(a)

    
    
