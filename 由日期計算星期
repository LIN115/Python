m_to_d=[31,28,31,30,31,30,31,31,30,31,30,31]
day_cycle=['Tuesday','Wednesday','Thursday','Friday','Saturday','Sunday','Monday']
while True:
    
    m,d=map(int, input().split())
    
    if m>12 or d>31:
        print("Error")
    elif m<1 or d<1:
        print("Error")
    else:
        for j in range(m-1):
                d += m_to_d[j]
        print(day_cycle[(d-1)%7])
